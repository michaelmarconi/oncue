oncue-agent {
	oncue {
		scheduler {
			path = "akka://oncue-service@localhost:9090"${oncue.scheduler.path}
		}
	}
	
	akka {
        actor {
            provider = "akka.remote.RemoteActorRefProvider"
        }
        remote {
        	transport = "akka.remote.netty.NettyRemoteTransport"
            netty {
	            hostname = "localhost"
	            port = 0
           }
        }
	}
}

